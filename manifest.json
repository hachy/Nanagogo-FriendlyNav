{
  "manifest_version": 2,
  "name": "Nanagogo Friendly Nav(Nogizaka46)",
  "description": "Nogizaka46 friendly navigation in 755(7gogo)",
  "version": "1.0.8",
  "permissions": [
    "https://7gogo.jp/*"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["https://7gogo.jp/*"],
      "js": ["jquery.js", "myscript.js"],
      "css": ["mystyle.css"]
    }
  ]
}
